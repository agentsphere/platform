  "query": "\n        SELECT u.id as \"user_id!\", u.name as \"user_name!\", u.is_active as \"is_active!\"\n        FROM auth_sessions s\n        JOIN users u ON u.id = s.user_id\n        WHERE s.token_hash = $1\n          AND s.expires_at > now()\n        ",
