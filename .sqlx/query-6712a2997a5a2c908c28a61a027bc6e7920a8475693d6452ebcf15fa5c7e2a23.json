  "query": "\n        SELECT p.id, p.name, p.resource, p.action, p.description\n        FROM permissions p\n        JOIN role_permissions rp ON rp.permission_id = p.id\n        WHERE rp.role_id = $1\n        ORDER BY p.name\n        ",
