{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            d.id,\n            d.delegator_id,\n            d.delegate_id,\n            d.permission_id,\n            p.name as \"permission_name!\",\n            d.project_id,\n            d.expires_at,\n            d.reason,\n            d.created_at,\n            d.revoked_at\n        FROM delegations d\n        JOIN permissions p ON p.id = d.permission_id\n        WHERE d.delegator_id = $1 OR d.delegate_id = $1\n        ORDER BY d.created_at DESC\n        ",
